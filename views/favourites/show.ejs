	<h2>Favoritos<span> de <%=session.user.username%></span></h2>
	<% if (quizes) { %>
	<table>
	  <% var i; for (i=0; i < quizes.length; i++) { %>
	    <tr>
	      <td><a href="/quizes/<%= quizes[i].id %>"><%= quizes[i].pregunta %></a></td>
		  <td>
			<form method="post" action="/user/<%= session.user.id %>/favourites/<%= quizes[i].id %>?_method=delete">
			  <input type="image" src="../../star_oro.png" onClick="return confirm('Eliminar: <%= quizes[i].pregunta %> de Favoritos');"></button>
			</form>
		  </td>
	    </tr>
	  <% } %>
	</table>
	<% } %>